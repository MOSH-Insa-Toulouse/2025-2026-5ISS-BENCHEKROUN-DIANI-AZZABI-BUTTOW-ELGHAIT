    {
        "id": "6df15d5f3ee58a26",
        "type": "function",
        "z": "3d35441da62ff479",
        "name": "function 2",
        "func": "if (!msg.payload || typeof msg.payload.value !== \"number\") return null;\n\nconst sensorId = msg.payload.sensorId;\nconst value = msg.payload.value;\n\n// Exemple: alerte seulement pour le gaz (sensorId = 0)\nlet alert = \"Normal\";\nif (sensorId === 0 && value > 80) alert = \"Help\";\n\nmsg.payload = { sensorId, value, alert };\nreturn msg;",
